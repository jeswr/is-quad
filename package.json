{
  "name": "is-quad",
  "version": "0.0.0-development",
  "description": "Checks if a BaseQuad is a valid RDF Quad",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/is-quad"
  },
  "keywords": [
    "Quad",
    "BaseQuad",
    "isQuad",
    "is-quad",
    "RDF",
    "utility"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/is-quad/issues"
  },
  "homepage": "https://github.com/jeswr/is-quad#readme",
  "devDependencies": {
    "@rdfjs/data-model": "^1.3.4",
    "@rdfjs/types": "^2.0.0",
    "@types/jest": "^27.0.3",
    "@types/rdf-data-model": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^9.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.3.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^24.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
